<package xmlns="http://schemas.microsoft.com/packaging/2015/06/nuspec.xsd">
  <metadata>
    <id>timestamper</id>
    <version>$version$</version>
    <owners>corbob</owners>
    <title>TimeStamper</title>
    <authors>TimeStamper Contributors</authors>
    <projectUrl>https://github.com/corbob/timestamper</projectUrl>
    <licenseUrl>https://raw.githubusercontent.com/corbob/timestamper/main/LICENSE</licenseUrl>
